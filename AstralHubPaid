        title.Size = UDim2.new(1, 0, 0, 40)
        title.BackgroundTransparency = 1
        title.TextColor3 = Color3.fromRGB(255, 255, 255)
        title.Text = "Select Speed"
        title.TextScaled = true
        title.Parent = sFrame

        local btn2x = Instance.new("TextButton")
        btn2x.Size = UDim2.new(0, 120, 0, 50)
        btn2x.Position = UDim2.new(0.5, -130, 0.5, -25)
        btn2x.Text = "2x Speed"
        btn2x.TextScaled = true
        btn2x.Parent = sFrame

        local btn3x = Instance.new("TextButton")
        btn3x.Size = UDim2.new(0, 120, 0, 50)
        btn3x.Position = UDim2.new(0.5, 10, 0.5, -25)
        btn3x.Text = "3x Speed"
