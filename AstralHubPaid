        if slotBtn then
            warn("[Placing] " .. unitName .. " from Slot " .. slot)
            remotes.PlaceUnit:InvokeServer(unitName, data)
        else
            warn("[Error] Slot " .. slot .. " not found")
        end
    end

    function startGame()
